<head>
    <%- include('include/head.ejs')%>

</head>


<body>
    <%- include('include/header.ejs')%>

    <main>
        <% profiles.forEach(function(profile){%>
        <h2><%= profile.name %></h2>

        <img src="/images/<%=profile.url %>" alt="">
        <article>
            <p><%=profile.age %></p>
            <p><%=profile.sports%></p>
            <p><%=profile.gender %></p>
            <p><%=profile.location %></p>
        </article>


        <% }) %>



    </main>
    <%- include('include/footer.ejs')%>


</body>

</html>